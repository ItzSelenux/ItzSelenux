key "Super+E" nemo 
key "Super+Space" rofi -show drun -font "comfortaa 13" -show-icons -location 7
key "Ctrl+Alt+T" sakura
key "Print" mate-screenshot -i
key "Ctrl+Alt+X" xkill

#windowkey

key "Super+Left" icesh -f sizeto 50% 100% left
key "Super+Right" icesh -f sizeto 50% 100% right 

key "Shift+Down" icesh -f sizeto 100% 50% bottom
key "Shift+Up" icesh -f sizeto 100% 50% top

key "Shift+Right" icesh -f sizeto 50% 50% top right
key "Ctrl+Shift+Left" icesh -f sizeto 50% 50% bottom left
key "Shift+Left" icesh -f sizeto 50% 50% top left
key "Ctrl+Shift+Right" icesh -f sizeto 50% 50% bottom right

key "Super+Down" icesh -f restore
key "Super+Up" icesh -f maximize
